{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentationt",
  "scripts": {
    "clean": "gulp clean",
    "build:client": "gulp build:client",
    "build:server": "gulp build:server",
    "build": "npm run clean && npm run build:client && npm run build:server && concurrently \"node dist/server/server.js\" \"npm start\"",
    "watch:server": "gulp watch:server",
    "compile": "gulp compile",
    "compile:server": "gulp compile:server",
    "watch": "gulp watch",
    "serve": "lite-server -c=bs-config.json",
    "start": "concurrently \"npm run watch\" \"npm run serve\"",
    "startserver":"node dist/server/server.js",
    "buildserver":"npm run clean && npm run build:server",
    "buildclient":"npm run clean && npm run build:server"

  },
